Class Vehicle
    Property name

    Function run(speed)
        Print name + " cannot run. It's just a base vehicle and don't know how to move!"
    End
End

Class Kart Of Type Vehicle
    Function run(speed)
        Print name + " running at " + speed + " km/h"
    End
End

Class Plane Of Type Vehicle
    Function run(speed)
        Print name + " taking off at " + speed + " km/h"
    End
End

vehicle = New Vehicle(name="Magic Vehicle")
vehicle.run(speed=80)

Print

mariokart = New Kart(name="Mario Kart")
mariokart.run(speed=15)

Print

plane = New Plane(name="Plane")
plane.run(speed=500)
